index = 9;
found = 0;
while (1 > found) {
	index = index - 1;
	b = 2;
	divider = 0;
	while (index > b){
		c = index;
		
		while (c - b*100+1 > 0){
			c = c - b*100
		};
		
		while (c - b+1 > 0){
			c = c - b
		};
	
		while (1>c){
			divider = 1;
			c = 1
		};
		b = b+1	
	};
	while (1 > divider){
		found = 1;
		divider = 1
	}
}
